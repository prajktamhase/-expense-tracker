 const getApiTransaction= async (req, res) => {
    const { amount, type, description, category } = req.body;

    const transaction = new Transaction({
        amount,
        type,
        description,
        category
    });
    try {
        const saveTransaction = await transaction.save();
        return res.json({
            success: true,
            message: 'Transaction save',
            data: saveTransaction
        })
    }
    catch (err) {
       return  res.json({
            success: false,
            message:err.message,
        })
    }
}
export {getApiTransaction}